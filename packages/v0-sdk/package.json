{
  "name": "v0-sdk",
  "version": "0.14.0",
  "description": "TypeScript SDK for the v0 Platform API",
  "homepage": "https://v0.dev/docs/api",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/v0-sdk.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:go": "tsgo --noEmit",
    "build": "bunchee",
    "clean": "rm -rf dist *.tsbuildinfo",
    "sdk:generate": "tsx src/scripts/generate.ts && pnpm format",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "echo 'No linting configured for v0-sdk package'",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "v0",
    "vercel",
    "ai",
    "chat",
    "sdk"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^22.12.0",
    "@typescript/native-preview": "7.0.0-dev.20250613.1",
    "bunchee": "^6.5.2",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "5.7.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}

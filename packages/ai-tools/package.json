{
  "name": "@v0-sdk/ai-tools",
  "version": "0.1.2",
  "description": "AI SDK tools for the v0 Platform API",
  "homepage": "https://v0.dev/docs/api",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/v0-sdk.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "bunchee",
    "clean": "rm -rf dist *.tsbuildinfo",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "echo 'No linting configured for ai-tools package'",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "v0",
    "vercel",
    "ai",
    "ai-sdk",
    "tools",
    "chat",
    "sdk"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "v0-sdk": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.12.0",
    "bunchee": "^6.5.2",
    "prettier": "^3.3.3",
    "typescript": "5.7.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "ai": "^5.0.0"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
